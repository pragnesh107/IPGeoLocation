<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>IP Lookup</title>
    <link rel="stylesheet" href="styles/style.css">
</head>
<body>
    <h1>Your IP Lookup</h1>
    <div class="container">
        <table>
            <thead>
                <tr>
                    <th>Title</th>
                    <th>Data</th>
                </tr>
            </thead>
            <tbody>
                <% if(locals.data && locals.data != "undefined"){ %>
                    <% Object.keys(data).forEach(key => { %>
                        <tr>
                            <td><%= key %></td>
                            <td><%= data[key] %></td>
                        </tr>
                    <% }); %>        
                <% } else { %>
                    <tr>
                        <td> title </td>
                        <td> body </td>
                    </tr>
                <% } %>
            </tbody>
        </table>
    </div>
</body>
</html>